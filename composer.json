{
  "name": "jield-webdev/azure-blob-storage",
  "description": "This project provides a set of PHP client libraries that make it easy to access Microsoft Azure storage APIs.",
  "keywords": [
    "php",
    "azure",
    "storage",
    "sdk"
  ],
  "version": "1.1.0",
  "license": "MIT",
  "require": {
    "php": "^8.3.0 || ^8.4.0",
    "guzzlehttp/guzzle": "^7.9",
    "ext-fileinfo": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-xsl": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "iteaoffice/testing": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "AzureOSS\\Storage\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AzureOSS\\Storage\\Tests\\": "tests/"
    }
  },
  "archive": {
    "exclude": [
      "/tests"
    ]
  }
}
